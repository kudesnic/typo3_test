{
    "name": "test/subscriber",
    "type": "typo3-cms-extension",
    "description": "Subscriber extension",
    "authors": [],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "Test\\Subscriber\\": "Classes"
        }
